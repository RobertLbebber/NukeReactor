{
  "name": "Electr",
  "version": "0.2.4 ",
  "private": true,
  "port": 1337,
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "t.jsx",
      "jsx"
    ],
    "modulePaths": [
      "**/*.{t.jsx,js,jsx}"
    ],
    "coverageDirectory": "src/coverage",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^2.0.3",
    "@material-ui/lab": "^1.0.0-alpha.12",
    "@types/googlemaps": "^3.0.0",
    "@types/markerclustererplus": "^2.1.29",
    "babel-jest": "^24.1.0",
    "babel-loader": "7.1.2",
    "bootstrap": "^4.2.1",
    "chai": "^4.2.0",
    "chai-dom": "^1.8.1",
    "chartist": "0.10.1",
    "cryptiles": ">=4.1.2",
    "hoek": ">=4.2.1",
    "jest": "^24.1.0",
    "jquery": "1.9.1 - 3",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "material-components-web": "^0.41.0",
    "node-gyp": "^3.8.0",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "0.0.3",
    "npm-install-peers": "^1.2.1",
    "ory-editor": "^0.4.4",
    "ory-editor-plugins-background": "^0.4.4",
    "ory-editor-plugins-html5-video": "^0.4.4",
    "ory-editor-plugins-slate": "^0.4.4",
    "react": "^16.5.2",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-chartist": "0.13.1",
    "react-cookie": "^3.0.8",
    "react-dnd": "^5.0.0",
    "react-dom": "^16.8.2",
    "react-dropzone": "^8.1.0",
    "react-google-maps": "9.4.5",
    "react-icons": "^3.3.0",
    "react-jss": "^8.6.1",
    "react-jsx-parser": "^1.5.1",
    "react-notification-system": "0.2.17",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.1",
    "react-tap-event-plugin": "^3.0.3",
    "react-test-renderer": "^16.8.2",
    "reactstrap": "^7.1.0",
    "slate": "^0.41.2",
    "slate-edit-blockquote": "0.6.1",
    "universal-cookie": "^3.0.7",
    "webpack": "3",
    "webpack-dev-middleware": "1.12.2",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/index.scss -o src/build/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/build/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "legacy-test": "react-scripts test --env=jsdom",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "css-loader": "2.1.0",
    "jest-config": "^24.1.0",
    "less": "^3.9.0",
    "npm-run-all": "^4.1.2",
    "selenium-webdriver": "^4.0.0-alpha.1"
  }
}
