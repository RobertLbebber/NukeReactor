  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via node",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${workspaceRoot}\\node_modules\\serverless\\bin\\serverless",
      "runtimeArgs": ["offline", "--port", "3001"],
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Serverless Offline",
      "program": "${workspaceRoot}/node_modules/serverless/bin/serverless",
      "args": ["offline", "--noTimeout", "--dontPrintOutput", "--port", "4000"],
      "sourceMaps": true,
      "runtimeArgs": ["--lazy"],
      "outFiles": ["${workspaceFolder}\\**\\*.js"],
      "protocol": "inspector",
      "runtimeExecutable": "node",
      "env": {},
      "windows": {
        "program": "${workspaceRoot}\\node_modules\\serverless\\bin\\serverless"
      },
      "sourceMapPathOverrides": {
        "*": "${webRoot}\\*"
      }
    }
  ]
}
